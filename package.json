{
  "name": "exjn",
  "version": "1.0.0",
  "description": "simple EXpression in JsoN for data extraction",
  "main": "dist/exjn.js",
  "types": "dist/exjn.d.ts",
  "files": [
    "dist/exjn.js",
    "dist/exjn.d.ts",
    "dist/exjn.min.js",
    "dist/exjn.min.js.map",
    "LICENSE",
    "README.md",
    "SYNTAX.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfam/exjn.js.git"
  },
  "keywords": [
    "text",
    "extraction",
    "regular",
    "expression"
  ],
  "author": "Ninh Pham",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/nfam/exjn.js/issues"
  },
  "homepage": "https://github.com/nfam/exjn.js#readme",
  "scripts": {
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build": "tsc",
    "minify": "node_modules/.bin/uglifyjs ./dist/exjn.js --comments --compress --source-map \"base='./dist'\"  -o ./dist/exjn.min.js",
    "test": "node_modules/.bin/nyc npm run test-ts",
    "test-ts": "node_modules/.bin/mocha -r ts-node/register ./test/*.test.ts",
    "coverage": "node_modules/.bin/nyc report --reporter=html",
    "coveralls": "node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/coveralls/bin/coveralls.js"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "test/*"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^2.2.46",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.2",
    "typescript": "^2.6.2",
    "uglify-js": "^3.3.7"
  }
}
